# docker-compose default environment variable values
COMPOSE_PROJECT_NAME=gscloud_dev
TAG=1.7.3
ACL_TAG=2.2.0
GS_USER="1000:1000"

BASE_PATH=/geoserver/cloud

GEOSERVER_DEFAULT_PROFILES="default,acl"

